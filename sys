import sys
import collections

inputs = sys.stdin.readline().strip().split(" ")
k = int(inputs[0])
d = int(inputs[1])

dna = []

for line in sys.stdin:
    dna.append(line.strip())

dnakmers = []

print(*dna)

for kmer in dna:
    print(kmer)
    for i in range(len(kmer)):
        print(kmer[i:i+k])
        dnakmers.append(kmer[i:i+k])

print(*dnakmers)

